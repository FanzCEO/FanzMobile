{
  "name": "fanzmobile",
  "version": "1.0.0",
  "description": "FANZ Mobile App - React Native application for creators and fans",
  "main": "index.js",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "supabase:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace FanzMobile.xcworkspace -scheme FanzMobile -configuration Release -destination generic/platform=iOS -archivePath FanzMobile.xcarchive archive",
    "clean": "react-native clean",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && rm -rf build && xcodebuild clean -workspace FanzMobile.xcworkspace -scheme FanzMobile",
    "postinstall": "cd ios && pod install",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-reanimated": "^3.6.2",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@supabase/supabase-js": "^2.39.0",
    "@react-native-firebase/app": "^19.0.1",
    "@react-native-firebase/messaging": "^19.0.1",
    "@react-native-firebase/analytics": "^19.0.1",
    "@react-native-firebase/crashlytics": "^19.0.1",
    "@react-native-firebase/auth": "^19.0.1",
    "@react-native-firebase/firestore": "^19.0.1",
    "@react-native-firebase/storage": "^19.0.1",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "@react-native-community/netinfo": "^11.2.0",
    "react-native-device-info": "^10.13.0",
    "react-native-permissions": "^4.1.2",
    "react-native-image-picker": "^7.1.0",
    "react-native-camera": "^4.2.1",
    "react-native-video": "^6.0.0-alpha.11",
    "react-native-webrtc": "^111.0.3",
    "react-native-webview": "^13.6.4",
    "react-native-svg": "^14.1.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-blur": "^4.3.2",
    "react-native-fast-image": "^8.6.3",
    "react-native-image-crop-picker": "^0.40.3",
    "lottie-react-native": "^6.4.1",
    "react-native-paper": "^5.12.3",
    "react-native-vector-icons": "^10.0.3",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-keychain": "^8.2.0",
    "react-native-share": "^10.0.2",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-qrcode-svg": "^6.3.0",
    "@walletconnect/react-native-dapp": "^1.8.0",
    "@web3auth/react-native-sdk": "^4.2.1",
    "ethers": "^6.10.0",
    "axios": "^1.6.5",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "yup": "^1.4.0",
    "date-fns": "^3.0.6",
    "react-native-toast-message": "^2.2.0",
    "react-native-modal": "^13.0.1",
    "react-native-actionsheet": "^2.4.2",
    "react-native-super-grid": "^4.9.6",
    "react-native-pull-to-refresh": "^2.1.3",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-charts-wrapper": "^0.5.11",
    "react-native-iap": "^12.13.3",
    "react-native-rate": "^1.2.12",
    "react-native-version-check": "^3.4.7",
    "react-native-code-push": "^8.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/runtime": "^7.24.0",
    "@react-native/babel-preset": "0.76.3",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.3",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.48",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^3.2.4",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.6.3",
    "detox": "^20.14.7",
    "@testing-library/react-native": "^12.4.3",
    "@testing-library/jest-native": "^5.4.3",
    "flipper-plugin-react-native-performance": "^2.0.0",
    "supabase": "^1.142.2"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FanzCEO/FanzMobile.git"
  },
  "keywords": [
    "react-native",
    "mobile-app",
    "creator-economy",
    "adult-content",
    "streaming",
    "nft",
    "web3",
    "blockchain",
    "ios",
    "android",
    "fanz"
  ],
  "author": "FANZ Development Team",
  "license": "PROPRIETARY",
  "bugs": {
    "url": "https://github.com/FanzCEO/FanzMobile/issues"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-navigation|@react-navigation|react-native-.*|@react-native-.*)/)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/index.ts"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "device": "iPhone 15 Pro",
        "app": "ios.debug"
      },
      "android.emu.debug": {
        "device": "android.emulator",
        "app": "android.debug"
      }
    },
    "apps": {
      "ios.debug": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/FanzMobile.app"
      },
      "android.debug": {
        "type": "android.apk",
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk"
      }
    },
    "devices": {
      "android.emulator": {
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_7_Pro_API_34"
        }
      }
    }
  }
}
