    {
      "name": "Upload Content",
      "short_name": "Upload",
      "description": "Quick upload new content",
      "url": "/?action=upload",
      "icons": [
        {
          "src": "/icons/shortcut-upload.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Schedule Post",
      "short_name": "Schedule",
      "description": "Schedule a new post",
      "url": "/?action=schedule",
      "icons": [
        {
          "src": "/icons/shortcut-schedule.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "View Analytics",
      "short_name": "Analytics",
      "description": "Check your performance",
      "url": "/?action=analytics",
      "icons": [
        {
          "src": "/icons/shortcut-analytics.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/?action=share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "media",
          "accept": ["image/*", "video/*"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/?action=upload",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif", ".webp", ".heic"],
        "video/*": [".mp4", ".mov", ".avi", ".webm", ".mkv"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+fanz",
      "url": "/?protocol=%s"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.fanz.mobile",
      "id": "com.fanz.mobile"
    },
    {
      "platform": "itunes",
      "url": "https://apps.apple.com/app/fanz-mobile/id123456789"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  }
}

